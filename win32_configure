  LIBS="-lpam $LIBS"

else
  as_fn_error $? "*** libpam missing" "$LINENO" 5
fi

			for ac_func in pam_getenvlist
do :
  ac_fn_c_check_func "$LINENO" "pam_getenvlist" "ac_cv_func_pam_getenvlist"
