# older BSDs need sys/param.h before sys/mount.h
for ac_header in sys/mount.h
do :
  ac_fn_c_check_header_compile "$LINENO" "sys/mount.h" "ac_cv_header_sys_mount_h" "
#include <sys/param.h>

"
